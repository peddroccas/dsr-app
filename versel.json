{
	"version": 2,
	"builds": [
		{
			"src": "admin/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "admin/dist"
			}
		},
		{
			"src": "manager/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "manager/dist"
			}
		}
	],
	"rewrites": [
		{ "source": "/manager/(.*)", "destination": "/manager/$1" },
		{ "source": "/admin/(.*)", "destination": "/admin/$1" }
	]
}
